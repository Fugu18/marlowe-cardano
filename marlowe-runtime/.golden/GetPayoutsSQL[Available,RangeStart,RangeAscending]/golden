Delimiter statement: SELECT payoutTxOut.txId, payoutTxOut.txIx, applyTx.slotNo FROM marlowe.payoutTxOut NATURAL JOIN marlowe.applyTx LEFT JOIN marlowe.withdrawalTxIn ON payoutTxOut.txId = withdrawalTxIn.payoutTxId AND payoutTxOut.txIx = withdrawalTxIn.payoutTxIx WHERE (payoutTxOut.txId = $1 AND payoutTxOut.txIx = $2) AND withdrawalTxIn.txId ISNULL
Total count statement: SELECT count(*) FROM marlowe.payoutTxOut NATURAL JOIN marlowe.applyTx LEFT JOIN marlowe.withdrawalTxIn ON payoutTxOut.txId = withdrawalTxIn.payoutTxId AND payoutTxOut.txIx = withdrawalTxIn.payoutTxIx WHERE (TRUE) AND withdrawalTxIn.txId ISNULL
Payouts statement: SELECT withdrawalTxIn.txId, applyTx.createTxId, applyTx.createTxIx, payoutTxOut.txId, payoutTxOut.txIx, payoutTxOut.rolesCurrency, payoutTxOut.role FROM marlowe.payoutTxOut NATURAL JOIN marlowe.applyTx LEFT JOIN marlowe.withdrawalTxIn ON payoutTxOut.txId = withdrawalTxIn.payoutTxId AND payoutTxOut.txIx = withdrawalTxIn.payoutTxIx WHERE (applyTx.slotNo > $1 OR (applyTx.slotNo = $1 AND (payoutTxOut.txId > $2 OR (payoutTxOut.txId = $2 AND payoutTxOut.txIx >= $3)))) AND withdrawalTxIn.txId ISNULL ORDER BY applyTx.slotNo, payoutTxOut.txId, payoutTxOut.txIx OFFSET $4 FETCH NEXT $5 ROWS ONLY
