Delimiter statement: WITH roleTokens (rolesCurrency, role) AS (SELECT * FROM UNNEST($1, $2)) SELECT payoutTxOut.txId, payoutTxOut.txIx, applyTx.slotNo FROM marlowe.payoutTxOut NATURAL JOIN marlowe.applyTx NATURAL JOIN roleTokens WHERE payoutTxOut.txId = $3 AND payoutTxOut.txIx = $4
Total count statement: WITH roleTokens (rolesCurrency, role) AS (SELECT * FROM UNNEST($1, $2)) SELECT count(*) FROM marlowe.payoutTxOut NATURAL JOIN marlowe.applyTx NATURAL JOIN roleTokens WHERE TRUE
Payouts statement: WITH roleTokens (rolesCurrency, role) AS (SELECT * FROM UNNEST($1, $2)) SELECT withdrawalTxIn.txId, applyTx.createTxId, applyTx.createTxIx, payoutTxOut.txId, payoutTxOut.txIx, payoutTxOut.rolesCurrency, payoutTxOut.role FROM marlowe.payoutTxOut NATURAL JOIN marlowe.applyTx NATURAL JOIN roleTokens LEFT JOIN marlowe.withdrawalTxIn ON payoutTxOut.txId = withdrawalTxIn.payoutTxId AND payoutTxOut.txIx = withdrawalTxIn.payoutTxIx WHERE applyTx.slotNo > $3 OR (applyTx.slotNo = $3 AND (payoutTxOut.txId > $4 OR (payoutTxOut.txId = $4 AND payoutTxOut.txIx >= $5))) ORDER BY applyTx.slotNo, payoutTxOut.txId, payoutTxOut.txIx OFFSET $6 FETCH NEXT $7 ROWS ONLY
